{
  "name": "@ember-learn/cli-guides",
  "version": "0.1.0",
  "private": true,
  "description": "Markdown files for Ember-Cli Guides",
  "bugs": {
    "url": "https://github.com/ember-learn/cli-guides-source/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ember-learn/cli-guides-source.git"
  },
  "license": "MIT",
  "author": "Jen Weber",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:alex": "alex .",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "lint:md": "remark .",
    "start": "ember serve",
    "test": "npm run test:node && npm run test:ember",
    "test:ember": "ember test",
    "test:node": "mocha node-tests"
  },
  "devDependencies": {
    "@ember/jquery": "^0.5.2",
    "@ember/optional-features": "^0.6.3",
    "alex": "^6.0.0",
    "broccoli-asset-rev": "^2.7.0",
    "ember-ajax": "^3.1.0",
    "ember-cli": "~3.5.0",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-babel": "^6.16.0",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-eslint": "^4.2.3",
    "ember-cli-fastboot": "^2.0.0",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^1.0.3",
    "ember-cli-inject-live-reload": "^1.8.2",
    "ember-cli-netlify": "0.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-cli-uglify": "^2.1.0",
    "ember-data": "~3.5.0",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^3.4.1",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.5.0",
    "ember-welcome-page": "^3.2.0",
    "eslint-plugin-ember": "^5.2.0",
    "guidemaker": "^0.6.1",
    "guidemaker-ember-template": "^0.5.0",
    "guidemaker-link-checker": "^1.0.0",
    "loader.js": "^4.7.0",
    "mocha": "^5.0.0",
    "prember": "^1.0.0",
    "qunit-dom": "^0.8.0",
    "remark-cli": "^5.0.0",
    "remark-lint": "^6.0.2",
    "remark-preset-lint-consistent": "^2.0.2",
    "remark-preset-lint-recommended": "^3.0.2"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      "remark-preset-lint-consistent",
      [
        "remark-lint-list-item-indent",
        "space"
      ]
    ]
  }
}
